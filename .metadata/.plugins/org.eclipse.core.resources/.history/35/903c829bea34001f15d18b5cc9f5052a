#include "DtDatosCompra.h"

DtDatosCompra::DtDatosCompra(validezCompra tipoSuscripcion,metodopagoCompra metodoCompra,int costo)
{
	this->tipoSuscripcion=tipoSuscripcion;
	this->metodopagoCompra=metodoCompra;
	this->costo=costo;

}

DtDatosCompra::~DtDatosCompra() {
	// TODO Esbozo de destructor generado automÃ¡ticamente
}

validezCompra DtDatosCompra::getTipoSuscripcion()
{
	return this->tipoSuscripcion;
}

metodoPagoCompra DtDatosCompra::getMetodoPagoCompra()
{
	return this->metodopagoCompra;
}

int DtDatosCompra::getCosto()
{
	return this->costo;
}
