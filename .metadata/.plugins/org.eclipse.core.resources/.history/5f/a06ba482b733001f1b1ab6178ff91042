#ifndef CLASES_MULTIJUGADOR_H_
#define CLASES_MULTIJUGADOR_H_

class Multijugador {
private:

int cantJugadores;
bool trasmitidaVivo;
int duracionTotal;

public:
	Multijugador();
	int getCantJugadores();
	bool getTrasmitidaVivo();
	int getDuracionTotal();
	virtual ~Multijugador();
};

#endif /* CLASES_MULTIJUGADOR_H_ */
