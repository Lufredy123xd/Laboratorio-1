ControladorVideojuego ControladorVideojuego::getInstanciaVideojuego()
{
	if (this->instanciaVideojuego==NULL)
		{
			this->instanciaVideojuego=new ControladorVideojuego();
		}else
		{
			return this->instanciaVideojuego;
		}
}

