#ifndef DATATYPES_DTFECHA_H_
#define DATATYPES_DTFECHA_H_

class DtFecha {
private:

	int anio;
	int mes;
	int dia;
	int hora;
	int minuto;
	int segundo;

public:
	DtFecha();

		int getAnio();
		int getMes();
		int getDia();
		int getHora();
		int getMinuto();
		int getSegundo();

		void setAnio(int);
		void setMes(int);
		void setDia(int);
		void setHora(int);
		void setMinuto(int);
		void setSegundo(int);

	virtual ~DtFecha();
};

#endif /* DATATYPES_DTFECHA_H_ */
